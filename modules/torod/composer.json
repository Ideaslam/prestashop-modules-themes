{
    "name": "secretcodes/torod",
    "description": "integration with torod system",
    "authors": [
        {
            "name": "Islam Hassan Abu Aish",
            "email": "islam.abuaish@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/translation": "^5.4",
        "symfony/dependency-injection": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "SecretCodes\\Module\\Torod\\": "src/",
            "SecretCodes\\Module\\Torod\\Services\\": "src/Services/" ,
            "SecretCodes\\Module\\Torod\\Exceptions\\": "src/Exceptions/"   ,
            "SecretCodes\\Module\\Torod\\Interfaces\\": "src/Interfaces/" ,
            "SecretCodes\\Module\\Torod\\Shared\\": "src/Shared/"  ,
            "SecretCodes\\Module\\Torod\\Entity\\": "src/Entity/"  
        },
        "classmap": [ 
            "src/"
        ],
         
        "exclude-from-classmap": []
    },
    "config": {
        "preferred-install": "dist",
        "prepend-autoloader": false
    },
    "type": "prestashop-module",
    "author": "SecretCodes",
    "license": "MIT"
}
